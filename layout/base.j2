<!DOCTYPE html>

<html lang="en">
<head>
{% block starthead %}{% endblock starthead %}
  <title>Setup SMTP Server on Postmark</title>
  <meta charset="{{ resource.meta.charset }}">
  <meta http-equiv="X-UA-Compatible" content="{{ resource.meta.compatibility }}">

  <title>{% block title %}{{ resource.meta.title }}{% endblock %}</title>
 

  <meta name="description" content="{{ resource.meta.description }}">
  <meta name="author" content="{{ resource.meta.author }}">
   {% block css %}
  <!--[if IE 7]>
  <link rel="stylesheet" href="{{ media_url('css/ie7.css') }}" media="all">
  <![endif]-->
  <link rel="stylesheet" href="{{ media_url('css/global.css') }}" media="all">
  {% endblock css %}

  {% block headjs %}
    <!--[if IE]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

  {% endblock headjs %}
  {% block endhead %}{% endblock endhead %}

</head>
<body id="{{ resource.meta.id if resource.meta.id else resource.slug }}">


  
  {% block content %}
<div class="page">
  <div class="container">
      {% block container %}
          <header class="banner clearfix">
          {% block header -%}
          <a class="home" href="{{ site.content.url }}"><h1 class="title">{{ site.meta.name }}</h1></a>
          {%- endblock %}
          </header>

        {% block nav %}
        {% from "nav.j2" import render_nav with context %}
        {{ render_nav(menu) }}
        {% endblock %}  
          <div id="docs">

        {% block main %}
        {% endblock main %}
        </div>

      </div>
    {% endblock container %}

  </div> <!--! end of #container -->
</div>

  {% endblock content%}
  {% block js %}
  <!-- Javascript at the bottom for fast page loading -->
  {% block jquery %}
  <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if necessary -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.js"></script>
  <script>window.jQuery || document.write('<script src="js/libs/jquery-1.5.1.min.js">\x3C/script>')</script>
  {% endblock jquery %}

  {% block scripts %}
  {% endblock scripts %}

  <script type="text/javascript"> Cufon.now(); </script>
  <!--[if lt IE 7 ]>
    <script src="js/libs/dd_belatedpng.js"></script>
    <script>DD_belatedPNG.fix('img, .png_bg'); // Fix any <img> or .png_bg bg-images. Also, please read goo.gl/mZiyb </script>
  <![endif]-->
        <div id="footer">
          Powered by <a href="http://github.com/hyde/hyde">hyde</a>.
        </div>

  {% endblock js %}
</body>
</html>








    
